{
  "name": "hugo_modules",
  "version": "0.0.2",
  "description": "modules for hugo",
  "main": "index.js",
  "scripts": {
    "server": "npm run clean && hugo server --disableFastRender",
    "build": "npm run clean && hugo --gc --minify",
    "clean": "rimraf public",
    "dev": "npm run clean && hugo",
    "mod:update": "hugo mod get -u ./...",
    "mod:dependencies": "hugo mod npm pack",
    "mod:graph": "hugo mod graph",
   
    "sprite": "gulp sprite --gulpfile gulp/gulpfile.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/powerfulwebdesign/hugo_modules.git"
  },
  "author": "Powerful Web Design",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/powerfulwebdesign/hugo_modules/issues"
  },
  "homepage": "https://github.com/powerfulwebdesign/hugo_modules#readme",

  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "autoprefixer": "^10.2.5",
    "postcss": "^8.2.8",
    "postcss-cli": "^8.3.1", 
    "gulp": "^4.0.2",
    "gulp-svg-sprite": "^1.5.0",
    "rimraf": "^3.0.2",
    "gulp-cheerio": "^1.0.0",
    "gulp-svgo": "^2.2.1"
  },
  "dependencies": {
    "swiper": "^6.4.15",
    "@popperjs/core": "^2.9.1",
    "bootstrap": "^5.0.0-beta2",
    "lazysizes": "^5.3.0"
  }
}
